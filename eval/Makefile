CC=		clang

RM=		rm -f

CFLAGS=		-Wall -Wextra -Werror

NAME=		eval

SRCS=		arbrebinairedetest.c \
		go_left.c

OBJS=		$(SRCS:.c=.o)

all:		$(NAME)

$(NAME):	$(OBJS)
		$(CC) $(OBJS) -o $(NAME) $(LDFLAGS)

clean:
		$(RM) $(OBJS)

fclean:		clean
		$(RM) $(NAME)

re:		fclean all

.PHONY:		all fclean re
